{
  "name": "dottxado/email-fields-for-woocommerce",
  "description": "Manage Reply To and BCC of the WooCommerce emails",
  "minimum-stability": "dev",
  "license": "GPL-2.0-or-later",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Erika Gili",
      "email": "ing.erika.gili@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "inpsyde/php-coding-standards": "^1.0.0",
    "phpunit/phpunit": "9.*",
    "brain/monkey": "*"
  },
  "autoload": {
    "psr-4": {
      "Dottxado\\EmailFieldsForWoocommerce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dottxado\\EmailFieldsForWoocommerce\\Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "tests": "@php ./vendor/phpunit/phpunit/phpunit"
  }
}
